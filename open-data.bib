journal = {Science},
keywords = {artificial intelligence,automation,computational biology,computers,enzymes,enzymology,fungal,genes,genetics,genomics,growth {\&} development,metabolism,programming languages,robotics,saccharomyces cerevisiae,software},
number = {5923},
pages = {85--89},
pmid = {19342587},
publisher = {AAAS},
title = {{The automation of science.}},
