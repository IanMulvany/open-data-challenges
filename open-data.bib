@article{Hughes2001,
abstract = {In this paper we present optimal orbits for the Laser Interferometer Space Antenna (LISA). LISA is a NASA mission to study gravity waves and test predictions of general relativity. The nominal formation consists of three spacecraft in heliocentric orbits at 1 AU and trailing the Earth by twenty degrees. This configuration was chosen as a trade off to reduce the noise sources that will affect the instrument and to minimize the fuel to achieve the final orbit. We present equations for the nominal orbit design and discuss several different measures of performance for the LISA formation. All of the measures directly relate the formation dynamics to science performance. Also, constraints on the formation dynamics due to spacecraft and instrument limitations are discussed. Using the nominal solution as an initial guess, the formation is optimized using Sequential Quadratic Programming to maximize the performance while satisfying a set of nonlinear constraints. Optimal solutions are presented for each of the performance measures.},
author = {Hughes, Steven P and Bauer, Frank H},
title = {{Optimal Orbit Design for the Laser Interferometer Space Antenna}},
year = {2001}
}
