keywords = {Cosmology and Extragalactic Astrophysics,cosmology},
mendeley-tags = {cosmology},
month = {jul},
pages = {6},
title = {{Dark Matter Contraction and the Stellar Content of Massive Early-type Galaxies: Disfavoring "Light" Initial Mass Functions}},
